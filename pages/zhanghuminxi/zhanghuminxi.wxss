/* pages/zhanghuminxi/zhanghuminxi.wxss */
.zhmx_list{
  width: 100%;
  height: 180rpx;
  padding: 30rpx 24rpx;
  margin-top: 2rpx;
  box-sizing: border-box;
}
.zhmx_list>image{
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  margin-right: 30rpx;
  display: block;
  float: left;
}
.neicont{
  width: 350rpx;
  height: 120rpx;
  display: inline-block;
}
.neicont>view{
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.gaodu{
  line-height: 120rpx;
}